{
  "name": "contact-list-app",
  "version": "0.1.0",
  "description": "Atividade Ser Educacional Back-end",
  "main": "./src/api/server.js",
  "engines": {
    "node": ">=14.0.0"
  },
  "engineStrict": true,
  "private": true,
  "scripts": {
