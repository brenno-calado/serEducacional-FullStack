{
  "name": "contact-list-app",
  "version": "0.1.0",
  "description": "Atividade Ser Educacional Front-end",
  "engines": {
    "node": ">=14.0.0"
  },
  "engineStrict": true,
  "private": false,
  "scripts": {
    "with-env": "SKIP_PREFLIGHT_CHECK=true",
    "start": "npm run with-env react-scripts start",
    "build": "npm run with-env react-scripts build",
    "eject": "npm run with-env react-scripts eject",
    "lint": "eslint --no-inline-config --no-error-on-unmatched-pattern -c .eslintrc.json .",
    "lint:styles": "stylelint **/*.css --config .stylelintrc.json --ignore-disables --allow-empty-input"
  },
  "dependencies": {
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "11.2.7",
    "@testing-library/user-event": "12.8.3",
    "axios": "^0.21.1",
    "moment": "^2.29.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "serve": "12.0.0",
    "web-vitals": "1.1.2"
  },
  "devDependencies": {
    "eslint-config-trybe-frontend": "1.2.1",
    "stylelint": "13.13.1",
    "stylelint-config-standard": "22.0.0",
    "stylelint-order": "4.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
